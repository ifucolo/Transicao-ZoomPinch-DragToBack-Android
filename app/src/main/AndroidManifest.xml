<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.com.dragbackzoompinchglide">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="example.com.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="example.com.dragbackzoompinchglide.activity.ZoomPinchGlideEndActivity"
            android:theme="@style/AppTheme.TransAll"/>

        <activity android:name="example.com.dragbackzoompinchglide.activity.ZoomPinchGlideStartActivity"/>

        <activity android:name="example.com.dragback.activity.DragBackEndActivity"
            android:theme="@style/AppTheme.TransAll"/>

        <activity android:name="example.com.dragback.activity.DragBackStartActivity"/>
    </application>

</manifest>